<!DOCTYPE html>
<head>
    <title> Record: Wages</title>
    <link href="/stylesheets/main.css" rel="stylesheet">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
       
</head>

<body>
    <!-- THIS IS THE HEADER -->
    <div class="heads">
  <nav class="navbar navbar-default" id="heads">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="\land"> <div class="navhead">Farm Bills</div></a>
      </div>
      <div class="collapse navbar-collapse">
        <ul class="nav navbar-nav navbar-right" id="heads">
             
                <li><a href="land">Properties</a></li>
                <!-- <li><a href="/">Sign Up</a></li> -->
           
         
        </ul>
      </div>
    </div>

</nav>
</div>


    <div id="cardblock">

    </div>
    <section>
        <!--for demo wrap-->
        <h1>Record: Wages</h1>
        <div class="tbl-header">
          <table cellpadding="0" cellspacing="0" border="0">
            <thead>
              <tr>
                <th>Date</th>
                <th>Employee</th>
                <th>Work Done</th>
                <th>Attendance</th>
                <th>Wage</th>
                <!-- <th>Performance</th> -->
              </tr>
            </thead>
          </table>
        </div>
        <div class="tbl-content">
          <table cellpadding="0" cellspacing="0" border="0">
            <tbody>
                <% wages.forEach(function(wage){ %>
                   
              <tr>
                <td><%= wage.date %></td>
                <td><%= wage.name %> </td>
                <td><%= wage.description %></td>
                <% if(wage.attendance ==='/') { %>
                    <td> Present  </td>
                <% } %>
                <% if(wage.attendance ==null) { %>
                    <td> Absent  </td>
                <% } %>
                
                
                <td><%= wage.amount %></td>
                <!-- <td><%= wage.amount %></td> -->
              </tr>
            
              <% }); %>
              

            </tbody>
          </table>
        </div>
      </section>
     

</body>

</html>