<%- include("../../partials/header") %>

  
        <!-- THIS IS THE BLOCKS FOR DISPLAYING PROPERTIES -->
        <div class="wrapper row3">
            <main class="hoc container clear"> 
              <!-- main body -->
              <!-- ################################################################################################ -->
              <section id="introblocks">
                <div class="sectiontitle">
                  <h6 class="heading">My Properties</h6>
                  <p>View all registered properties here:</p>
                </div>
                <article>
                    <footer><a class="btn" href="/land/add"> Add New Property</a></footer>
                
                </article>
               <div class="container">
                <!-- <div class="row"> -->
                
                <ul class="nospace group">
                    <% lands.forEach(function(land){ %>
                        <div class="col-md-4">
                  <li class="one_third " id="cardblock">
                    <article><a href="#"><i class="fa fa-home"></i></a>
                      <h6 class="heading"><%= land.name %></h6>
                      <p><%= land.location %> </p>
                      <p><%= land.description %>  </p>
                      <footer><a class="btn" id="edit" href="#">Edit</a>
                        <form class="delete-form" action="/land/<%=land._id%>?_method=DELETE" method="POST">
                          <input type="submit" class="btn btn-xs btn-danger" id="delete" value="Delete"> 
        
                        </form>
                        <!-- <a class="btn" action="/land/<%=land._id%>?_method=DELETE" method="POST" id="delete" href="#">Delete</a></footer> -->
                    </article>
                  </li>
                  <% }); %>
                </ul>
            </div>
        </div>
    </div>
              </section>
            </main>
            </div>
       
            <%- include("../../partials/footer") %>